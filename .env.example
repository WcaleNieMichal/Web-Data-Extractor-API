# Skopiuj do .env.{dev|test|prod} i dostosuj wartości
# Użycie: ENV=prod python main.py

# Środowisko: dev | test | prod
ENV=dev

# Aplikacja
DEBUG=True
LOG_LEVEL=INFO

# Baza danych
DATABASE_URL=sqlite:///data/scraper.db

# Scraping
REQUEST_TIMEOUT=30
REQUEST_DELAY=1.0
MAX_RETRIES=3
MAX_CONCURRENT_REQUESTS=5

# Proxy (opcjonalnie)
PROXY_URL=
